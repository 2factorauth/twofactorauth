---
---
{
{%- for section in site.data.sections -%}
 {%- assign section_file = site.data[section.id] -%}"{{ section.title }}":{
 {%- assign websites = section_file.websites | where_exp: "item", "item.tfa != nil" -%}
    {%- for website in websites -%}"{{ website.name }}":{
      {%- for tag in website -%}
        {%- if tag[0] == "name" -%}
          {%- continue -%}
        {%- endif -%}
        {%- unless tag[0] == "exceptions" -%}"{{ tag[0] }}":
          {%- unless tag[0] == "tfa" -%}
            "{{tag[1]}}"
          {%- else -%}
            [
            {%- for method in tag[1] -%}
              "{{ method }}"
              {%- unless forloop.last -%},{%- endunless -%}
            {%- endfor -%}
            ]
          {%- endunless  -%}
        {%- else -%}
          "{{ tag[0] }}":{
          {%- for exception in tag[1] -%}
            "{{ exception[0] }}":"{{ exception[1] }}"
            {%- unless forloop.last -%},{%- endunless -%}
          {%- endfor -%}
          }
        {%- endunless -%}
        {%- unless forloop.last -%},{%- endunless -%}
      {%- endfor -%}
      }
      {%- unless forloop.last -%},{%- endunless -%}
    {%- endfor -%}
  }
  {%- unless forloop.last -%},{%- endunless -%}
{%- endfor -%}
}
